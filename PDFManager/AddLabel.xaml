<Window x:Class="PDFManager.AddLabel"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="AddLabel" Height="147" Width="248" ResizeMode="NoResize" WindowStyle="None" BorderThickness="1" BorderBrush="Chocolate" Background="Cornsilk">
    <Grid>
        <TextBox Height="37" Margin="30,10,30,0" Name="textBox_label" VerticalAlignment="Top" KeyDown="textBox_label_KeyDown" />
        <Button Content="确认" Height="23" HorizontalAlignment="Left" Margin="20,70,0,0" Name="button_ok" VerticalAlignment="Top" Width="75" Click="button_ok_Click" />
        <Button Content="取消" Height="23" Margin="0,70,20,0" Name="button_cancel" VerticalAlignment="Top" HorizontalAlignment="Right" Width="75" Click="button_cancel_Click" />
    </Grid>
</Window>
